{
  "realm": "studyhelper",
  "enabled": true,
  "registrationAllowed": false,
  "resetPasswordAllowed": true,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "clients": [
    {
      "clientId": "studyhelper-spa",
      "name": "StudyHelper SPA",
      "rootUrl": "",
      "protocol": "openid-connect",
      "publicClient": true,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "redirectUris": [
        "http://localhost:8081/*"
      ],
      "webOrigins": [
        "http://localhost:8081"
      ],
      "attributes": {
        "pkce.code.challenge.method": "S256"
      }
    },
    
    {
      "clientId": "nicegui-app",
      "name": "NiceGUI App",
      "protocol": "openid-connect",
      "publicClient": false,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "redirectUris": [
        "http://localhost:8081/auth/callback"
      ],
      "webOrigins": [
        "http://localhost:8081"
      ],
      "secret": "nicegui-secret",
      "attributes": {
        "pkce.code.challenge.method": "S256",
        "post.logout.redirect.uris": "http://localhost:8081/*",
        "frontchannel.logout": "true"
      }
    },
    {
      "clientId": "oauth2-proxy",
      "name": "OAuth2 Proxy",
      "protocol": "openid-connect",
      "publicClient": false,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "rootUrl": "",
      "redirectUris": [
        "http://localhost:8081/oauth2/callback"
      ],
      "webOrigins": [
        "http://localhost:8081"
      ],
      "secret": "oauth2-proxy-secret",
      "attributes": {
        "pkce.code.challenge.method": "S256",
        "post.logout.redirect.uris": "http://localhost:8081/*",
        "frontchannel.logout": "true"
      }
    }
  ],
  "users": [
    {
      "username": "alice",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Alice",
      "lastName": "Tester",
      "email": "alice@example.com",
      "credentials": [
        {
          "type": "password",
          "value": "alice",
          "temporary": false
        }
      ]
    }
  ]
}
